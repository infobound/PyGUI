{
	"Toolbar": {
		"tkinter": {
			"Select": "",
			"Button": {
				"properties": "button.json"
			},
			"Label": {
				"properties": "label.json"
			},
			"Entry": {
				"properties": "entry.json"
			},
			"Frame": {
				"properties": "frame.json"
			},
			"Canvas": {
				"properties": "canvas.json"
			}
		},
		"Special": {
			"Scrollable Frame": {
				"properties": {
					"base": {
						"library": {
							"type": "label",
							"default": "Special"
						},
						"class": {
							"type": "label",
							"default": "Special"
						},
						"name": {
							"type": "text",
							"default": "ScrollableFrame"
						},
						"placement manager": {
							"type": "Select",
							"options": {
								"Place": "place",
								"Pack": "pack",
								"Grid": "grid"
							},
							"default": "pack"
						}
					},
					"attributes": {
						"activebackground": {
							"type": "color",
							"description": "Background color when the button is under the cursor."
						},
						"activeforeground": {
							"type": "color",
							"description": "Foreground color when the button is under the cursor."
						},
						"highlightcolor": {
							"type": "color",
							"description": "The color of the focus highlight when the widget has focus."
						},
						"bd": {
							"type": "int",
							"min": 0,
							"description": "Border width in pixels. Default is 2."
						},
						"command": {
							"type": "text",
							"description": "Function or method to be called when the button is clicked."
						},
						"fg": {
							"type": "color",
							"description": "Normal foreground (text) color."
						},
						"font": {
							"type": "font",
							"description": "Text font to be used for the button's label."
						},
						"image": {
							"type": "file",
							"description": "Image to be displayed."
						},
						"padx": {
							"type": "int",
							"min": 0,
							"description": "Additional padding left and right of the text."
						},
						"pady": {
							"type": "int",
							"min": 0,
							"description": "Additional padding above and below the text."
						},
						"text": {
							"type": "text",
							"default": "New Form"
						}
					},
					"geometry": {
						"y": {
							"type": "int",
							"min": 0,
							"default": 0
						},
						"x": {
							"type": "int",
							"min": 0,
							"default": 0
						},
						"height": {
							"type": "int",
							"min": 50,
							"default": 500
						},
						"width": {
							"type": "int",
							"min": 50,
							"default": 500
						}
					}
				},
				"script": ""
			}
		}
	}
}

